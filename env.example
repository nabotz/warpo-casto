# Environment Configuration
# Copy this file to .env and fill in your actual values

# Server Configuration
PORT=3000

# Discord Webhook Configuration
# Get this from your Discord server settings > Integrations > Webhooks
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/YOUR_WEBHOOK_ID/YOUR_WEBHOOK_TOKEN

# Neynar Webhook Configuration
# Get this from your Neynar dashboard webhook settings
NEYNAR_WEBHOOK_SECRET=your_neynar_webhook_secret_here

# Optional: Filter specific users (comma-separated)
# FILTER_FID=12345,67890
# FILTER_USERNAME=username1,username2

# Logging Configuration
LOG_LEVEL=info
